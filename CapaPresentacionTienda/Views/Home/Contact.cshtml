

@{
    ViewBag.Title = "Contact";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<br />
<br />
<div id="carouselExampleCaptions" class="carousel slide carousel-dark w-25 mx-auto" data-bs-ride="carousel">

    <div class="carousel-indicators">
        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
    </div>
    <div class="carousel-inner">

        <div class="carousel-item active">

            <img src="~/Content/imag/Screenshot_20240922-180653-fotor-bg-remover-2024092218838.png" class="d-block w-100" alt="cafe" />
        </div>
        <div class="carousel item">

            <img src="~/Content/icono/pngwing.com (1).png" class="d-block w-100" alt="sorve" />
        </div>
        <div class="carousel item">

            <img src="~/Content/icono/Screenshot_20240630-124646-fotor-bg-remover-20240921174945.png" class="d-block w-100" alt="sorve" />
        </div>

    </div>

    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
        <span class="carousel-control-prev-icon text-black" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
        <span class="carousel-control-next-icon text-black" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>

</div>
<br />
<br />